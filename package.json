{
  "name": "react-nails",
  "version": "0.2.24",
  "description": "UI framework based on React and Styled Components",
  "author": "Auskennfuchs",
  "license": "MIT",
  "repository": "git@github.com:Auskennfuchs/react-nails.git",
  "homepage": "https://auskennfuchs.github.io/react-nails/",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd react-nails-storybook && yarn install && yarn run build-storybook",
    "deploy": "gh-pages -d react-nails-storybook/storybook-static"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/react-fontawesome": "^0.1.7"
  },
  "peerDependencies": {
    "lodash": "^4.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^4.0.0"
  },
  "devDependencies": {
    "@svgr/rollup": "^2.4.1",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^23.1.5",
    "@types/lodash": "^4.14.146",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/styled-components": "^4.4.0",
    "cross-env": "^5.1.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "gh-pages": "^1.2.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts-ts": "^2.16.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-typescript2": "^0.17.0",
    "rollup-plugin-url": "^1.4.0",
    "styled-components": "^4.4.1",
    "typescript": "^3.3.4000"
  },
  "files": [
    "dist"
  ]
}
